<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="HeaderTextAppearance">
        <item name="android:gravity">center</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">@dimen/_18ssp</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="PromptTextAppearance">
        <item name="android:textColor">@color/grey</item>
        <item name="android:textSize">@dimen/_10ssp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textAllCaps">true</item>
    </style>

    <style name="RegularButton">
        <item name="android:textAllCaps">true</item>
        <item name="android:textColor">@color/white</item>
        <item name="cornerRadius">@dimen/_4sdp</item>
    </style>

</resources>